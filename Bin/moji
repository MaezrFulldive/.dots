# ~/bin/moji
#!/usr/bin/env bash

quotes=(
    "(Ëµ>á—œ <Ëµ)"
    "(Ë¶Ë˜ Â³Ë˜)â™¡"
    "(Ë¶Ëƒáµ•Ë‚Ë¶)"
    "â—(áµ”á—œ áµ”)â—œ"
    "(Ë¶áµ”áµ•áµ”Ë¶)â€¹ðŸ¹"
    "â‚^..^â‚Žâ³Š"
)

# Pick a random quote
